<!doctype html>
<html>
    <head>
        <meta charset="utf-8" />
        <title>Bertie's Books - Users</title>
        <link rel="stylesheet"  type="text/css" href="/main.css" />
    </head>
    <body>
        <div class="panel">
            <h1>Bertie's Books Shop</h1>
            <h2>Registered users</h2>

            <ul class="book-list">
<% users.forEach(function(user){ %>
            <li><strong><%= user.username %></strong> - <%= user.first %> <%= user.last %> (<%= user.email %>)</li>
            <% }) %>
            </ul>
        </div>
    </body>
</html>
